{
    "name": "Dictionary for Firefox",
    "version": "3",
    "manifest_version": 2,
    "description": "Searches wiktionary and wikipedia",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentscript.js"]
        }
    ],
    "permissions": ["*://*.wikipedia.org/*", "*://*.wiktionary.org/*"]
}