{
    "name": "wikidictionary",
    "version": "2",
    "manifest_version": 2,
    "description": "Searches wiktionary and wikipedia when you double click",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentscript.js"]
        }
    ],
    "permissions": ["*://*.wikipedia.org/*", "*://*.wiktionary.org/*"]
}